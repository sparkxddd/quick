PORT=3001
NODE_ENV=development

# Redis
REDIS_URL=redis://localhost:6379
# For production on Render, use: REDIS_URL=redis://your-redis-host:6379

# Security Secrets (Must be 32+ chars)
JWT_ACCESS_SECRET=replace_this_with_a_super_long_secret_string_32_chars
JWT_REFRESH_SECRET=replace_this_with_another_super_long_secret_string_32_chars

# External Services
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
RAZORPAY_KEY_ID=your_razorpay_key_id
RAZORPAY_KEY_SECRET=your_razorpay_key_secret

# Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_KEY=your_supabase_service_role_key
